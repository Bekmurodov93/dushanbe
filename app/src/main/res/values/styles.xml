    <style name="outlinedTextField" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="hintTextColor">@color/darkGrey</item>
        <item name="errorTextColor">@color/redWarning</item>
        <item name="endIconTint">@color/alifGreen</item>
        <item name="lineHeight">15.13sp</item>
    </style>
    <!-- Online identification -->
    <style name="online_ident_show_example_tv">
        <item name="android:textColor">@color/alifGreen</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="lineHeight">16sp</item>
    </style>

    <style name="online_ident_step_num_tv">
        <item name="android:enabled">false</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/online_identification_step_num_color</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
    <style name="online_ident_step_text">
        <item name="android:enabled">false</item>
        <item name="android:textColor">@color/online_identification_text_color</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="lineHeight">16sp</item>
    </style>


    <style name="online_ident_button_text">
        <item name="fontFamily">@font/roboto_bold</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/online_identification_button_text_color</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="online_ident_img_preview">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

    <style name="progressbar" parent="Widget.AppCompat.ProgressBar">
        <item name="colorAccent">@color/white</item>
    </style>

